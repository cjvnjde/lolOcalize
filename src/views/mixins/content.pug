include row

mixin content(entries)
    table#content
        thead
            tr
                th key
                th text
                th
                    button(
                        type="button",
                        hx-get="/open-add",
                        hx-target="body",
                        hx-swap="beforeend"
                    ) Add
        tbody
            each value in entries
                +row(value[0], value[1])
